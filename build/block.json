{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/dynamic-blog-sidebar-block",
  "version": "0.1.0",
  "title": "Dynamic Blog Sidebar Block",
  "category": "widgets",
  "icon": "smiley",
  "description": "To make navigating through the h2 tags on the page easier, dynamically get them from the post text.",
  "example": {},
  "attributes": {
    "h2tags": {
      "type": "array"
    },
    "postId": {
      "type": "number"
    }
  },
  "supports": {
    "color": {
      "background": false,
      "text": true
    },
    "html": false,
    "typography": {
      "fontSize": false
    }
  },
  "textdomain": "dynamic-blog-sidebar-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}